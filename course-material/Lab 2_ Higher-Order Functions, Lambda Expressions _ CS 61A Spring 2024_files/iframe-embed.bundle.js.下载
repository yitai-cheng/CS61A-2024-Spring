eval("/* provided dependency */ var $ = __webpack_require__(/*! jquery */ \"./js/lib/jquery-3.0.0.min.js\");\n\n// Copyright (C) Philip Guo\nObject.defineProperty(exports, \"__esModule\", ({ value: true }));\nexports.AiHelpWidget = exports.annotateCodeWithLineNumbers = exports.formatStackTraceWithLineNumbers = exports.createStackTraceWithLineNumbers = void 0;\nvar opt_frontend_common_1 = __webpack_require__(/*! ./opt-frontend-common */ \"./js/opt-frontend-common.ts\");\nvar pytutor_1 = __webpack_require__(/*! ./pytutor */ \"./js/pytutor.ts\");\n__webpack_require__(/*! ./lib/jquery-3.0.0.min.js */ \"./js/lib/jquery-3.0.0.min.js\");\n__webpack_require__(/*! ./lib/jquery.ba-dotimeout.min.js */ \"./js/lib/jquery.ba-dotimeout.min.js\"); // for $.doTimeout\n__webpack_require__(/*! ../css/ai-help.css */ \"./css/ai-help.css\");\n// for Tingle.js https://tingle.robinparisi.com/\nvar tingle = __webpack_require__(/*! ./lib/tingle.min.js */ \"./js/lib/tingle.min.js\");\n__webpack_require__(/*! ./lib/tingle.min.css */ \"./js/lib/tingle.min.css\"); // don't forget the accompanying CSS file!\n// thresholds for when to issue warnings:\nvar CODE_TOO_LONG_THRESHOLD = 2000; // 20 // small val for testing\nvar PROMPT_TOO_LONG_THRESHOLD = 4000; // 40 // small val for testing\n// version number for logging\n/* first version (v1) released on 2023-08-30, git commit hash: e57819caf2342df8f6e10a3da02643444e143261\n\n  - only supports \"Get AI Help\" in edit mode (i.e., when code editor is on-screen),\n    which captures the user's current code and, optionally, a compile-time error message\n    - no support for \"Get AI Help\" in visualize mode yet\n  - for exact wording of preseeded options in v1, look at git commit hash e57819caf2342df8f6e10a3da02643444e143261\n  - video walkthrough demo of v1: https://youtu.be/EcA0tsysGhQ\n*/\n//const AIHELP_VERSION = 'v1';\n/* v2 released on 2023-09-13, git commit hash: bb61c143dbb83d28a611f3abfd5b244752a5709f\n  - now supports \"Get AI Help\" in visualize mode as well as in edit mode\n  - captures the stack trace, line numbers of each stack frame, and current line number\n    in the prompt, along with runtime error message (if any)\n  - however, still does *not* capture stdout/stdin and values of variables, so users\n    will need to still manually write those into their prompts\n*/\nvar AIHELP_VERSION = 'v2';\n/* BEGIN functions that can be unit-tested offline separate from the frontend */\n// export these so that they can be tested offline in Node.js with a test runner\n// - maybe use golden_test.py as the test runner since it's fairly robust by now\n// returns an array of [function name, line number] pairs containing the current\n// contents of the stack at curStep in trace, with the active (is_highlighted)\n// function as the first element of array. use '<module>' as the fake function\n// name for top-level module execution outside of any function\n//\n// trace - array, a trace created by an OPT backend\n// curStep - current step number in the trace, should be in-bounds of trace\n// lang - language name, see constructor of ExecutionVisualizer for legal values\n//        (subtly different than those in langSettingToLanguageDetails since,\n//         for instance, 'py36' represents Python 3.6, and not simply '3')\nfunction createStackTraceWithLineNumbers(trace, curStep, lang) {\n    pytutor_1.assert($.isArray(trace) && 0 <= curStep && curStep < trace.length);\n    var curEntry = trace[curStep];\n    // as a consistency check, this should match the line number in the is_highlighted frame (or <module> if there's no frame)\n    var curLine = curEntry.line;\n    // Key: frame_id, Value: line number of most recently executed line in that frame\n    // - use '<module>' as the fake frame_id for top-level execution outside any function\n    //   since that's what it's called in Python, so for other languages like\n    //   JavaScript, we'll just use this same name for simplicity\n    var frameIdToMostRecentLineNumber = new Map();\n    // it seems like the Java backend (written by David Pritchard) doesn't use\n    // frame_id in the way that i originally intended, so *don't* rely on\n    // frame_id in Java traces. instead, see below for parsing the line\n    // number out of func_name, e.g., \"main:53\" is main() function on line 53\n    if (lang !== 'java') {\n        // very important to keep counting only until we reach curStep\n        // because we want to stop right at the current entry ...\n        for (var i = 0; i <= curStep; i++) {\n            var e = trace[i];\n            var myLine = e.line;\n            var myStack = e.stack_to_render;\n            // if there's no myStack, credit this line to '<module>':\n            if (!myStack || myStack.length === 0) {\n                frameIdToMostRecentLineNumber.set('<module>', myLine);\n            }\n            else {\n                // find top-most non-zombie entry using is_highlighted since that should\n                // be the currently-active stack frame (which the frontend highlights) \n                var highlightedEntry = myStack.find(function (entry) { return entry.is_highlighted; });\n                if (highlightedEntry && highlightedEntry.frame_id) {\n                    frameIdToMostRecentLineNumber.set(highlightedEntry.frame_id, myLine);\n                }\n            }\n        }\n    }\n    // now build a stack trace using all the (non-zombie) stack frames and the\n    // current line numbers at each frame:\n    var ret = [];\n    var stackToRender = curEntry.stack_to_render;\n    if (stackToRender) {\n        var isTopActiveFrame = true;\n        for (var i = curEntry.stack_to_render.length - 1; i >= 0; i--) {\n            var e = curEntry.stack_to_render[i];\n            // skip all zombie frames since they aren't part of the active stack:\n            if (e.is_zombie) {\n                continue;\n            }\n            if (lang === 'java') {\n                // parse line number from func_name since the Java backend embeds\n                // line numbers in func_name for each frame. e.g., \"foo:34\" or \"main:62\"\n                var match = e.func_name.match(/(.*):(\\d+)$/);\n                if (match) {\n                    var daFunc = match[1].trim();\n                    var daLine = Number(match[2]);\n                    if (isTopActiveFrame) {\n                        pytutor_1.assert(e.is_highlighted && curLine === daLine);\n                    }\n                    ret.push([daFunc, daLine]);\n                }\n            }\n            else {\n                var daLine = frameIdToMostRecentLineNumber.get(e.frame_id);\n                if (daLine) {\n                    if (isTopActiveFrame) {\n                        pytutor_1.assert(e.is_highlighted && curLine === daLine);\n                    }\n                    ret.push([e.func_name, daLine]);\n                }\n            }\n            isTopActiveFrame = false;\n        }\n    }\n    // get info about top-level <module> (i.e., execution outside any function)\n    // if there's a line number. (there's no top-level <module> for C, C++, Java\n    // since there should always be a 'main' function on the stack, i think)\n    var moduleLineNum = frameIdToMostRecentLineNumber.get('<module>');\n    if (moduleLineNum && moduleLineNum > 0) {\n        ret.push(['<module>', moduleLineNum]);\n    }\n    return ret;\n}\nexports.createStackTraceWithLineNumbers = createStackTraceWithLineNumbers;\n// stackTrace is the return value from createStackTraceWithLineNumbers\n// and format it as a string that can be included into an LLM prompt\nfunction formatStackTraceWithLineNumbers(stackTrace) {\n    return stackTrace.map(function (e) { return \"- \" + e[0] + \" at line \" + e[1]; }).join('\\n');\n}\nexports.formatStackTraceWithLineNumbers = formatStackTraceWithLineNumbers;\n// returns a string containing the contents of code but with each\n// line annotated with comments containing a line number and optional\n// annotation. this will hopefully help LLMs make better associations between\n// line numbers mentioned in the prompt and actual lines in the code\n//\n// code - input code\n// annotationsMap - ES6 Map, key: line number, value: annotation string (maybe '')\n//   - annotates lines given by line numbers with comments '// line 5'\n//     and appends optional annotation string to each line annotation\n// isPython - if it's true, then use '#' for comments, otherwise use '//'\nfunction annotateCodeWithLineNumbers(code, annotationsMap, isPython) {\n    var COMMENT = isPython ? '#' : '//';\n    var lines = code.split('\\n');\n    annotationsMap.forEach(function (ann, lineNum) {\n        // remember that lineNum is one-indexed, so don't be off-by-one\n        if (lineNum <= lines.length) {\n            lines[lineNum - 1] += \" \" + COMMENT + \" line \" + lineNum + (ann ? ann : '');\n        }\n    });\n    return lines.join('\\n');\n}\nexports.annotateCodeWithLineNumbers = annotateCodeWithLineNumbers;\n/* END utility functions for external unit testing */\nvar langSettingToLanguageName = {\n    '2': 'Python',\n    '3': 'Python',\n    '311': 'Python',\n    'js': 'JavaScript',\n    'ts': 'TypeScript',\n    'java': 'Java',\n    'c': 'C',\n    'cpp': 'C++',\n    'c_gcc9.3.0': 'C',\n    'cpp_g++9.3.0': 'C++',\n    'py3anaconda': 'Python',\n};\nvar langSettingToLanguageDetails = {\n    '2': 'Python 2.7',\n    '3': 'Python 3.6',\n    '311': 'Python 3.11',\n    'js': 'JavaScript ES6',\n    'ts': 'TypeScript',\n    'java': 'Java 8',\n    'c': 'C17 with GNU C extensions',\n    'cpp': 'C++20 with GNU C++ extensions',\n    'c_gcc9.3.0': 'C17 with GNU C extensions',\n    'cpp_g++9.3.0': 'C++20 with GNU C++ extensions',\n    'py3anaconda': 'Python 3.6',\n};\n// these are the 'legacy' settings for lang for backend scripts and logging,\n// *not* the ones passed into ExecutionVisualizer (yeah it's confusing, i know)\nfunction isLangPython(lang) {\n    return (lang === '2' || lang ==